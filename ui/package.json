{
  "name": "netfyffe-ui",
  "version": "0.0.1",
  "description": "User interface to access portfolio.",
  "scripts": {
    "build": "bin/build_config.js && tsc -p src/",
    "build:watch": "bin/build_config.js && tsc -p src/ -w",
    "serve": "lite-server -c=bs-config.js",
    "prestart": "npm run build",
    "start": "concurrently \"npm run build\" \"npm run serve\"",
    "dev": "concurrently \"npm run build:watch\" \"npm run serve\"",
    "lint": "tslint ./src/**/*.ts -t verbose"
  },
